{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.0",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/auto_height": "2.0.x-dev@dev",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^5.2",
        "drupal/block_class": "^1.3",
        "drupal/bootstrap_layouts": "^5.2",
        "drupal/bxslider": "^1.0",
        "drupal/config_direct_save": "^1.0",
        "drupal/console": "^1.9",
        "drupal/core": "^8.8.5",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/devel": "^4.1",
        "drupal/ds": "^3.12",
        "drupal/editor_advanced_link": "^1.8",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/entityqueue": "^1.2",
        "drupal/flexslider": "^2.0",
        "drupal/geolocation": "^3.3",
        "drupal/google_analytics": "^2.5",
        "drupal/google_map_field": "^1.14",
        "drupal/imce": "^2.4",
        "drupal/insert": "^2.0@beta",
        "drupal/libraries": "^3.0@beta",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/metatag": "^1.16",
        "drupal/module_filter": "^3.2",
        "drupal/nodeorder": "^1.0@beta",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/text_summary_options": "^1.2",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_tweak": "^2.9",
        "drupal/upgrade_status": "^3.14",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bootstrap": "^3.6",
        "drupal/views_conditional": "^1.4",
        "drupal/views_infinite_scroll": "^1.8",
        "drupal/views_slideshow": "^4.8",
        "drupal/views_taxonomy_term_name_into_id": "^1.0@alpha",
        "drupal/webform": "^6.1",
        "drupal/weight": "^3.3",
        "drush-ops/behat-drush-endpoint": "^9.3",
        "drush/drush": "~10.4",
        "platformsh/config-reader": "^2.4",
        "rvtraveller/qs-composer-installer": "^1.1",
        "typo3/phar-stream-wrapper": "3.1.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
